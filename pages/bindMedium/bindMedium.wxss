.main{
  width: 100%;
  height: 100%;
  text-align: center;
}
.sel{
  width: 100%;
  height: 60%;
  text-align: center;
}
.success{
  width: 100%;
  height: 60%;
  padding: 20rpx;
  text-align: left;
  font-size: 30rpx;
}
.success view{
  margin-top: 40rpx;
}
.img{
  width: 545rpx;
  height: 300rpx;
  display: block;
  margin: 0 auto;
  margin-top: 40rpx;
}
.h2{
  display: block;
  margin-top: 80rpx;
  font-size: 30rpx;
}
.inp{
  width: 100%;
  display: flex;
  padding: 0 20rpx;
  margin-top: 40rpx;
  box-sizing: border-box;
}
.inp input{
  border-bottom: 2rpx solid #ccc;
  margin-left: 20rpx;
  flex: 1;
}
button{
  width: 90%;
  margin-top: 40rpx;
}
.btn{
  background: blue;
  color: #fff;
}
.nav{
  width: 90%;
  margin-top: 40rpx;
  border: 2rpx solid #ccc;
  margin-left:auto;
  margin-right:auto;
  padding-left:14px;
  padding-right:14px;
  box-sizing:border-box;
  font-size:18px;
  text-align:center;
  text-decoration:none;
  line-height:2.55555556;
  border-radius:5px;
  -webkit-tap-highlight-color:transparent;
  overflow:hidden;
}