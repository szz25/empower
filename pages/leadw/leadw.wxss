.main{
  width: 100%;
  height: 100%;
  background: #eee;
  display: flex;
  flex-direction: column;
}
.head{
  width: 100%;
  height: 300rpx;
  background: #eee;
  display: flex;
  padding: 20rpx;
  box-sizing: border-box;
  justify-content: space-between;
  align-items: center;
}
.head view text:nth-child(2){
  font-size: 30rpx;
}
.quan{
  width: 100rpx;
  height: 100rpx;
  border: 2rpx solid #000;
  display: block;
  text-align: center;
  line-height: 100rpx;
  border-radius: 50%;
}
.gao{
  background: #ccc;
}
.user{
  width: 100%;
  background: #fff;
}
.essay{
  background: #fff;
  width: 100%;
  flex: 1;
  margin-top: 20rpx;
  display: flex;
  flex-direction: column;
}
.title{
  padding: 40rpx 40rpx;
  box-sizing: border-box;
  display: block;
  border-bottom: 2rpx solid #ccc;
}
.wen{
  flex: 1;
  width: 100%
}
.one{
  width: 100%;
  padding: 20rpx;
  border-bottom: 2rpx solid #ccc;
  box-sizing: border-box;
}
.content{
  display: flex;
  justify-content: space-between;
  font-size: 30rpx;
}
.tit{
  font-weight: bold;
  display: block;
  width: 60%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}